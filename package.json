{
  "name": "@neurolint/cli",
  "version": "1.0.0",
  "description": "Advanced rule-based code analysis and transformation tool using AST parsing and sophisticated pattern matching",
  "main": "src/index.js",
  "bin": {
    "neurolint": "./neurolint"
  },
  "scripts": {
    "start": "node neurolint",
    "dev": "node src/index.js",
    "build": "node scripts/build.js",
    "test": "echo 'CLI tests would run here'",
    "clean": "rm -rf dist/ build/",
    "package": "npm pack"
  },
  "keywords": [
    "code-analysis",
    "ast",
    "typescript",
    "react",
    "nextjs",
    "cli",
    "code-transformation",
    "linting",
    "fixing"
  ],
  "author": "NeuroLint",
  "license": "MIT",
  "homepage": "https://neurolint.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/Alcatecablee/nurolint"
  },
  "bugs": {
    "url": "https://neurolint.dev/support"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@babel/parser": "^7.28.0",
    "@vitejs/plugin-react": "^4.6.0",
    "axios": "^1.6.2",
    "boxen": "^7.1.1",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "glob": "^10.3.10",
    "ora": "^8.0.1",
    "vite": "^7.0.2",
    "ws": "^8.16.0"
  },
  "files": [
    "bin/",
    "src/",
    "scripts/",
    "README.md",
    "LICENSE"
  ],
  "preferGlobal": true
}
